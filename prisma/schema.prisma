generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum EnableState {
  Enable
  Disable
}

enum ContractType {
  FullTime
  PartTime
  Internship
  Temporary
  Freelance
}

enum CustomerType {
  Owner
  CM
  MainConstruction
  DesignerArchitect
  EndUser
  Dealer
}

enum PartnerType {
  CUSTOMER
  SUPPLIER
}

